{
  "name": "project-architect",
  "version": 1.0,
  "builds": [
    {
      "src": "project_architect.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/project_architect.py"
    }
  ],
  "functions": {
    "project_architect.py": {
      "runtime": "python3.11"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/project_architect.py" }
  ]
}